{
  "name": "ysite2/autoloader",
  "description": "Fastest autoloader",
  "type": "library",
  "version": "1.0.2",
  "keywords": [
    "autoloader"
  ],
  "require-dev": {
    "phpunit/phpunit": "11.4.x-dev"
  },
  "license": "BSD-2-Clause",
  "autoload": {
    "classmap": ["src/"]
  },
  "autoload-dev": {
    "psr-4": {
      "YRV\\Autoloader\\Tests\\": "Tests/"
    }
  },
  "authors": [
    {
      "name": "Roman Yankovenko",
      "email": "roman@yankovenko.ru"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": ">=8.0"
  },
  "scripts": {
    "test": "phpunit ./Tests"
  }
}
